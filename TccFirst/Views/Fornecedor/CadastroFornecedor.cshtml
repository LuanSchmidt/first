<div id="modal-fornecedor" class="modal fade" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Cadastrar Fornecedor</h5>
                <button type="button" class="close" data-dismiss="modal" arial-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <label for="fornecedor-campo-razaoSocial">Razão Social</label>
                    <input type="text" id="fornecedor-nome-razaoSocial" class="form-control" />
                </div>
                <div class="form-group">
                    <label for="fornecedor-campo-nomeFantasia">Nome Fantasia</label>
                    <input type="text" id="fornecedor-campo-nomeFantasia" class="form-control" />
                </div>
                <div class="form-group">
                    <label for="fornecedor-campo-dataCadastro">Data de Cadastro</label>
                    <input type="date" id="fornecedor-campo-dataCadastro" class="form-control" />
                </div>
                <div class="form-group">
                    <label for="fornecedor-campo-cnpj">CNPJ</label>
                    <input type="text" id="fornecedor-campo-cnpj" class="form-control" />
                </div>
                <div class="form-group">
                    <label for="fornecedor-campo-email">E-mail</label>
                    <input type="text" id="fornecedor-campo-email" class="form-control" />
                </div>
                <div class="form-group">
                    <label for="fornecedor-campo-telefone">Telefone</label>
                    <input type="text" id="fornecedor-campo-telefone" class="form-control" />
                </div>
                <div class="form-group">
                    <label for="fornecedor-campo-cep">Cep</label>
                    <input type="text" id="fornecedor-campo-cep" class="form-control" />
                </div>
                <div class="form-group">
                    <label for="fornecedor-campo-lofradouro">Logradouro</label>
                    <input type="text" id="fornecedor-campo-logradouro" class="form-control" />
                </div>
                <div class="form-group">
                    <label for="fornecedor-campo-numero">Numero</label>
                    <input type="text" id="fornecedor-campo-numero" class="form-control" />
                </div>
                <div class="form-group">
                    <label for="fornecedor-campo-bairro">Bairro</label>
                    <input type="text" id="fornecedor-campo-bairro" class="form-control" />
                </div>
                <div class="form-group">
                    <label for="fornecedor-campo-cidade">Cidade</label>
                    <input type="text" id="fornecedor-campo-cidade" class="form-control" />
                </div>
                <div class="form-group">
                    <label for="fornecedor-campo-sigla">Sigla do Estado</label>
                    <input type="text" id="fornecedor-campo-sigla" class="form-control" />
                </div>
                <div class="form-group">
                    <label for="fornecedor-campo-complemento">Complemento</label>
                    <input type="text" id="fornecedor-campo-complemento" class="form-control" />
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancelar</button>
                <button type="button" class="btn btn-primary" id="fornecedor-batao-salvar">Salvar</button>
            </div>
        </div>
    </div>
</div>
