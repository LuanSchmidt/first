
<h4>Cadastro de Agencias</h4>
<br />

<form action="~/Agencia/Cadastro">
    <div class="container">
        <div class="form-group">
            <label for="agencia-campo-banco">Banco<span class="text-danger">*</span></label>
            <input type="text" id="agencia-campo-banco" class="form-control" />
        </div>
        <div class="form-group">
            <label for="agencia-campo-nomeagencia">Nome da Agencia<span class="text-danger">*</span></label>
            <input type="text" id="agencia-campo-nomeagencia" class="form-control" />
        </div>
        <div class="form-group">
            <label for="campo-agencia-numeroagencia" mas>Numero da Agencia<span class="text-danger">*</span></label>
            <input type="text" id="agencia-campo-numeroagencia" class="form-control" />
        </div>

        <div class="modal-footer">
            <button type="button" class="btn btn-primary" id="agencia-botao-salvar" data-dismiss="modal">Salvar</button>

            <button type="button" class="btn btn-danger" id="agencia-botao-cancelar">Cancelar</button>

        </div>
    </div>
</form>


