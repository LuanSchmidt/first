<div id="modal-parcelaReceber" class="modal" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Cadastro de Parcelas Receber</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">

                <div class="form-group">
                    <label for="parcelaReceber-campo-tituloReceber" mas>Titulo Receber<span class="text-danger">*</span></label>
                    <select class="form-control"id="parcelasReceber-campo-tituloReceber"name="idTituloReceber"></select>
                </div>
                <div class="form-group">
                    <label for="parcelaReceber-campo-valor" mas>Valor<span class="text-danger">*</span></label>
                    <input type="text" class="form-control" id="parcelasReceber-campo-valor" />
                </div>

                <div class="form-group">
                    <label for="parcelaReceber-campo-status" mas>Status<span class="text-danger">*</span></label>
                    <select class="form-control" id="parcelasReceber-campo-status">
                        <option>Pendente</option>
                        <option>Pago</option>
                        <option>Cancelado</option>
                        <option>Pago parcialmente</option>
                    </select>
                </div>
                <div class="form-group">
                    <label for="parcelaReceber-campo-dataVencimento" mas>Data Vencimento<span class="text-danger">*</span></label>
                    <input type="date" class="form-control" id="parcelasReceber-campo-dataVencimento" />
                </div>
                <div class="form-group">
                    <label for="parcelaReceber-campo-dataRecebimento" mas>Data Recebimento<span class="text-danger">*</span></label>
                    <input type="date" class="form-control" id="parcelasReceber-campo-dataRecebimento" />
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancelar</button>
                    <button type="button" class="btn btn-primary" id="parcelasReceber-botao-salvar">Salvar</button>
                </div>
            </div>
        </div>
    </div>
</div>

    
       

 
        

        

        
        
 