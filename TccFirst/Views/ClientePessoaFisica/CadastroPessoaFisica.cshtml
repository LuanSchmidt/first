                <div class="form-group">
                    <label for="clientePessoaFisica-campo-bairro">Bairro</label>
                    <input type="text" id="clientePessoaFisica-campo-bairro" class="form-control" />
                </div>
                <div class="form-group">
                    <label for="clientePessoaFisica-campo-cidade">Cidade</label>
                    <input type="text" id="clientePessoaFisica-campo-cidade" class="form-control" />
                </div>
                <div class="form-group">
                    <label for="clientePessoaFisica-campo-uf">UF</label>
                    <input type="text"id="clientePessoaFisica-campo-uf" class="form-control"/>
                </div>
                <div class="form-group">
                    <label for="clientePessoaFisica-campo-complemento">Complemento</label>
                    <input type="text" id="clientePessoaFisica-campo-complemento" class="form-control" />
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline-danger" data-dismiss="modal">Cancelar</button>
                <button type="button" class="btn btn-success" id="clientePessoaFisica-batao-salvar">
                    <i class="fa fa-floppy-o"></i> Salvar
