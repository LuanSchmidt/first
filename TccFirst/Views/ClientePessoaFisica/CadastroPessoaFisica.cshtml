<div id="modal-clientePessoaFisica" class="modal" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Cadastrar Cliente pessoa Fisica</h5>
                <button type="button" class="close" data-dismiss="modal" arial-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <span id="msg-error"></span>
                <div class="row">
                    <div class="col-6">
                        <div class="form-group">
                            <span id="msg-error"></span>
                            <label for="clientePessoaFisica-campo-nome">Nome</label>
                            <input type="text" id="clientePessoaFisica-campo-nome" class="form-control" placeholder="Nome" />
                        </div>
                    </div>
                    <div class="col-6">
                        <div class="form-group">
                            <label for="clientePessoaFisica-campo-cpf">Cpf</label>
                            <input type="text" id="clientePessoaFisica-campo-cpf" class="form-control" />
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-6">
                        <div class="form-group">
                            <label for="clientePessoaFisica-campo-dataNascimento">Data de Nascimento</label>
                            <input type="date" id="clientePessoaFisica-campo-dataNascimento" class="form-control" />
                        </div>
                    </div>
                    <div class="col-6">
                        <div class="form-group">
                            <label for="clientePessoaFisica-campo-limiteCredito">Limite de Crédito</label>
                            <input type="text" id="clientePessoaFisica-campo-limiteCredito" class="form-control" />
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <label for="clientePessoaFisica-campo-email">E-mail</label>
                    <input type="text" id="clientePessoaFisica-campo-email" class="form-control" />
                </div>
                <div class="form-group">
                    <label for="clientePessoaFisica-campo-telefone">Telefone</label>
                    <input type="text" id="clientePessoaFisica-campo-telefone" class="form-control" />
                </div>
                <div class="form-group">
                    <label for="clientePessoaFisica-campo-cep">Cep</label>
                    <input type="text" id="clientePessoaFisica-campo-cep" class="form-control" />
                </div>
                <div class="form-group">
                    <label for="clientePessoaFisica-campo-rua">Rua</label>
                    <input type="text"id="clientePessoaFisica-campo-rua" class="form-control"/>
                </div>
                <div class="form-group">
                    <label for="clientePessoaFisisca-campo-numero">Numero</label>
                    <input type="text" id="clientePessoaFisica-campo-numero" class="form-control" />
