
@{
    ViewBag.Title = "Index";
}

<h2>Clientes Pessoa Fisica</h2>
<button data-toggle="modal" data-target="#modal-clientePessoaFisica" class="btn btn-primary">Novo Cliente Pessoa Fisica</button>
<table id="cliente-pessoa-fisica-tabela" class="table table-hover table-bordered table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nome</th>
            <th>Cpf</th>
            <th>Data Nascimento </th>
            <th>Limite de Crédito</th>
            <th>E-mail</th>
            <th>Telefone</th>
            <th>Cep</th>
            <th>Número</th>
            <th>Bairro</th>
            <th>Cidade</th>
            <th>Uf</th>
            <th>Complemento</th>
            <th>Ação</th>
        </tr>
    </thead>
</table>
@RenderPage("CadastroPessoaFisica.cshtml")

@section js{
    <script src="~/Scripts/ClientePessoaFisica/cliente-pessoa-fisica-index.js"></script>
}
