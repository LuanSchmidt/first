
@{
    ViewBag.Title = "Index";
}
<div class="container">
    <h2><b>Cliente Pessoa Juridica</b></h2>
    <hr />
    <div class="row">
        <div class="col-12">
            <button data-toggle="modal" id="botao-novoCliente" data-target="#modal-clientePessoaJuridica" class="btn float-right">
                <i class="fa fa-plus"></i>Novo Cliente
            </button>
        </div>
    </div>
    <br />
    <div class="col-12">

        <table id="cliente-pessoa-juridica-tabela" class="table table-striped table-bordered table-hover">
            <thead>
                <tr>
                    <th>Codigo</th>
                    <th>Razão Social</th>
                    <th>Nome Fantasia</th>
                    <th>Cnpj</th>
                    <th>UF</th>
                    <th>Cidade</th>
                    <th>Ação</th>
                </tr>

            </thead>
        </table>
    </div>
</div>
@RenderPage("CadastroPessoaJuridica.cshtml")
<link rel="stylesheet" href="~/Content/css/ClientePessoaJuridica/TelaIndexPessoaJuridica.css">
@section js {
    <script src="~/Scripts/ClientePessoaJuridica/cliente-pessoa-juridica.js"></script>
}
