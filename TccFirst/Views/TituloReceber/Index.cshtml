
@{
    ViewBag.Title = "Index";
}

<h2>Lista de Títulos a Receber</h2>
<a href="/tituloreceber/cadastro" class="btn btn-info float-right mb-3">
    <i class="fa fa-plus"></i>Novo Titulo a Receber
</a>

    <table id="tituloReceber-tabela" class="table table-hover table-bordered table-striped">
        <thead>
            <tr>
                <th>Código</th>
                <th>Cliente</th>
                <th>Valor</th>
                <th>Quantidade de parcelas</th>
                <th>Status</th>
                <th>Data de Lançamento</th>
                <th>Data de Recebimento</th>
                <th>Data de Vencimento</th>
                <th>Complemento</th>
                <th>Descrição</th>
                <th>Ação</th>
            </tr>
        </thead>
    </table>

    
   
    @section js{
        <script src="~/Scripts/TituloReceber/tabela-receber-index.js"></script>
        
    }

