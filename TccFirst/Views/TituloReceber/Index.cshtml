
@{
    ViewBag.Title = "Index";
}

<h2>Cadastro de Títulos a Receber</h2>
<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#modal-tituloReceber">
    Criar título a Receber
</button>
<table id="tituloReceber-tabela" class="table table-hover table-bordered table-striped">
    <thead>
        <tr>
            <th>Código</th>
            <th>Cliente</th>
            <th>Valor</th>
            <th>Quantidade de parcelas</th>
            <th>Status</th>
            <th>Data de Lançamento</th>
            <th>Data de Recebimento</th>
            <th>Data de Vencimento</th>
            <th>Descrição</th>
            <th>Ação</th>
        </tr>
    </thead>
</table>
@RenderPage("CadastroTituloReceber.cshtml")
@section js{
    <script src="~/Scripts/TituloReceber/titulo-receber-cadastro.js"></script>
    <script src="~/Scripts/TituloReceber/titulo-receber-editar.js"></script>
}

