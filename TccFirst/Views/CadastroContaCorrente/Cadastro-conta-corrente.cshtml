

<div id="modal-cadastro-conta-corrente" class="modal fade" tabindex="-1" role="dialog" style="z-index: 1050;">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Cadastrar Titulo a receber</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <label for="cadastro-conta-corrente-campo-idAgencia">Agencia</label>
                    <select class="form-control" id="cadastro-conta-corrente-campo-idAgencia"></select>
                </div>

                <div class="form-group">
                    <label for="cadastro-conta-corrente-campo-numero-conta" mas>Numero da Conta Corrente</label>
                    <input type="text" id="cadastro-conta-corrente-campo-numero-conta" class="form-control" />
                </div>

            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancelar</button>
                <button type="button" class="btn btn-primary" id="cadastro-conta-corrente-botao-salvar">Salvar</button>
            </div>
        </div>
    </div>
</div>
@section js {
    <script src="~/Scripts/CadastroContaCorrente/Cadastro-conta-corrente-Editar.js"></script>
    <script src="~/Scripts/CadastroContaCorrente/cadastro-conta-corrente-cadastro.js"></script>
}